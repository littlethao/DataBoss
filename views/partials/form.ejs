<h3>Form</h3>
<div class="divider"></div>

<form class="" action="" method="post">

  <div class="row">

      <div class="col s4">
          <h5>Language</h5>
      </div>

      <div class="input-field col s8" id="input-box">
        <select id="language" name="language">
        </select>
      </div>

      <div class="col s4">
          <h5>Framework</h5>
      </div>
      <div class="input-field col s8" id="input-box">
        <select id="framework" name="framework">
        </select>
      </div>

      <div class="col s4">
          <h5>ORM</h5>
      </div>
      <div class="input-field col s8" id="input-box">
        <select id="ORM" name="orm">
        </select>
      </div>

    <div class="col s4">
        <h5>Table Name</h5>
    </div>
    <div class="input-field col s8" id="input-box">
      <input id="table-name" type="text" class="validate" name="tableName">
      <label for="table-name">Table Name</label>
    </div>

  <div class="col s12">
    <input type="Submit" class="btn right">
  </div>

</div>

</form>
