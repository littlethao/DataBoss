<div class="row valign-wrapper" id="form-title">
  <div class="col s9">
    <h3 class="valign">Form</h3>
  </div>
  <div class="col s3">
    <i class="material-icons valign medium" id="form-slide-in">swap_horiz</i>
  </div>
</div>

<div class="divider"></div>

<form class="" action="" method="post">

  <div id="form-content" class="row">

      <div class="col s5">
          <h5>Language</h5>
      </div>

      <div class="input-field col s7" id="input-box">
        <select id="language" name="language">
        </select>
      </div>

      <div class="col s5">
          <h5>Framework</h5>
      </div>
      <div class="input-field col s7" id="input-box">
        <select id="framework" name="framework">
        </select>
      </div>

      <div class="col s5">
          <h5>ORM</h5>
      </div>
      <div class="input-field col s7" id="input-box">
        <select id="ORM" name="orm">
        </select>
      </div>

    <div class="col s5">
        <h5>Table Name</h5>
    </div>
    <div class="input-field col s7" id="input-box">
      <input id="table-name" type="text" class="validate" name="tableName">
      <label for="table-name">Table Name</label>
    </div>

  <div class="col s6 offset-s3">
    <button class="btn waves-effect waves-light" id="form-btn">
      <input type="Submit">
      <i class="material-icons right">send</i>
    </button>
  </div>

</div>

</form>
